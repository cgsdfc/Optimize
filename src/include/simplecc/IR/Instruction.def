/// Definitions of Instruction
#ifndef HANDLE_INSTRUCTION
#define HANDLE_INSTRUCTION(Class, Opcode, Name)
#endif

#ifndef HANDLE_TERMINATOR
#define HANDLE_TERMINATOR(Class, Opcode, Name) HANDLE_INSTRUCTION(Class, Opcode, Name)
#endif

#ifndef HANDLE_BINARY_OPERATOR
#define HANDLE_BINARY_OPERATOR(Class, Opcode, Name) HANDLE_INSTRUCTION(Class, Opcode, Name)
#endif

#ifndef HANDLE_MEMORY_INST
#define HANDLE_MEMORY_INST(Class, Opcode, Name) HANDLE_INSTRUCTION(Class, Opcode, Name)
#endif

#ifndef HANDLE_ICMP_PREDICATE
#define HANDLE_ICMP_PREDICATE(Class, Opcode, Name)
#endif

HANDLE_TERMINATOR(ReturnInst, Ret, "ret")
HANDLE_TERMINATOR(BranchInst, Br, "br")

HANDLE_BINARY_OPERATOR(BinaryOperator, Add, "add")
HANDLE_BINARY_OPERATOR(BinaryOperator, Sub, "sub")
HANDLE_BINARY_OPERATOR(BinaryOperator, Mul, "mul")
HANDLE_BINARY_OPERATOR(BinaryOperator, Div, "div")

HANDLE_MEMORY_INST(AllocaInst, Alloca, "alloca")
HANDLE_MEMORY_INST(LoadInst, Load, "load")
HANDLE_MEMORY_INST(StoreInst, Store, "store")
HANDLE_MEMORY_INST(GetElementPtrInst, GEP, "getelementptr")

HANDLE_INSTRUCTION(ICmpInst, ICmp, "icmp")
HANDLE_INSTRUCTION(CallInst, Call, "call")
HANDLE_INSTRUCTION(PHINode, PHI, "phi")

HANDLE_ICMP_PREDICATE(ICmpInst, EQ, "eq")
HANDLE_ICMP_PREDICATE(ICmpInst, NEQ, "neq")
HANDLE_ICMP_PREDICATE(ICmpInst, SLT, "slt")
HANDLE_ICMP_PREDICATE(ICmpInst, SLE, "sle")
HANDLE_ICMP_PREDICATE(ICmpInst, SGT, "sgt")
HANDLE_ICMP_PREDICATE(ICmpInst, SGE, "sge")

#undef HANDLE_TERMINATOR
#undef HANDLE_BINARY_OPERATOR
#undef HANDLE_MEMORY_INST
#undef HANDLE_ICMP_PREDICATE
#undef HANDLE_INSTRUCTION