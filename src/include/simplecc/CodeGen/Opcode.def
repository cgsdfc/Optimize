// Definitions of Opcode
#ifndef HANDLE_OPCODE
#define HANDLE_OPCODE(opcode, camelName)
#endif

// Memory load and store.
HANDLE_OPCODE(LOAD_LOCAL, LoadLocal)
HANDLE_OPCODE(LOAD_GLOBAL, LoadGlobal)
HANDLE_OPCODE(LOAD_CONST, LoadConst)
HANDLE_OPCODE(LOAD_STRING, LoadString)
HANDLE_OPCODE(STORE_LOCAL, StoreLocal)
HANDLE_OPCODE(STORE_GLOBAL, StoreGlobal)

// Binary arithmetic
HANDLE_OPCODE(BINARY_ADD, BinaryAdd)
HANDLE_OPCODE(BINARY_SUB, BinarySub)
HANDLE_OPCODE(BINARY_MULTIPLY, BinaryMultiply)
HANDLE_OPCODE(BINARY_DIVIDE, BinaryDivide)

// Unary arithmetic.
HANDLE_OPCODE(UNARY_POSITIVE, UnaryPositive)
HANDLE_OPCODE(UNARY_NEGATIVE, UnaryNegative)

// Call and return.
HANDLE_OPCODE(CALL_FUNCTION, CallFunction)
HANDLE_OPCODE(RETURN_VALUE, ReturnValue)
HANDLE_OPCODE(RETURN_NONE, ReturnNone)

// Output.
HANDLE_OPCODE(PRINT_STRING, PrintString)
HANDLE_OPCODE(PRINT_CHARACTER, PrintCharacter)
HANDLE_OPCODE(PRINT_INTEGER, PrintInteger)
HANDLE_OPCODE(PRINT_NEWLINE, PrintNewline)

// Input.
HANDLE_OPCODE(READ_INTEGER, ReadInteger)
HANDLE_OPCODE(READ_CHARACTER, ReadCharacter)

// Array subscription.
HANDLE_OPCODE(BINARY_SUBSCR, BinarySubscr)
HANDLE_OPCODE(STORE_SUBSCR, StoreSubscr)

// Conditional and unconditional jump.
HANDLE_OPCODE(JUMP_IF_TRUE, JumpIfTrue)
HANDLE_OPCODE(JUMP_IF_FALSE, JumpIfFalse)
HANDLE_OPCODE(JUMP_FORWARD, JumpForward)
HANDLE_OPCODE(JUMP_IF_NOT_EQUAL, JumpIfNotEqual)
HANDLE_OPCODE(JUMP_IF_EQUAL, JumpIfEqual)
HANDLE_OPCODE(JUMP_IF_GREATER, JumpIfGreater)
HANDLE_OPCODE(JUMP_IF_GREATER_EQUAL, JumpIfGreaterEqual)
HANDLE_OPCODE(JUMP_IF_LESS, JumpIfLess)
HANDLE_OPCODE(JUMP_IF_LESS_EQUAL, JumpIfLessEqual)

// Others
HANDLE_OPCODE(POP_TOP, PopTop)

#undef HANDLE_OPCODE